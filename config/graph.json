{
    "root": {
        "children": ["load_ref"],
        "class": "IdentityNode",
        "module": "identitynode"
    },
    "load_ref": {
        "children": ["load_deg"],
        "class": "LoadSignalNode",
        "module": "loadsignalnode",
        "signal_path": "resources/reference/ref.wav",
        "output_key": "reference_signal",
        "target_sample_rate": 16000,
        "mono": "true"
    },
    "load_deg": {
        "children": ["align_signals"],
        "class": "LoadSignalNode",
        "module": "loadsignalnode",
        "signal_path": "resources/degraded/deg.wav",
        "output_key": "degraded_signal",
        "target_sample_rate": 16000,
        "mono": "true"
    },
    "align_signals" : {
        "output_key": "align_signals",
        "children": ["py_pesq_score"],
        "class": "AlignmentNode",
        "module": "alignmentnode"
    },
    "py_pesq_score" : {
        "output_key": "py_pesq_score",
        "children": ["blank"],
        "class": "PypesqNode",
        "module": "pypesqnode",
        "target_sample_rate": 16000,
        "pesq_mode": "wb"
    },
    "blank": {
        "children": ["blank_"],
        "class": "IdentityNode",
        "module": "identitynode"
    },
    "blank_": {
        "children": [],
        "class": "IdentityNode",
        "module": "identitynode"
    }
}